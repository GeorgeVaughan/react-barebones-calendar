{
  "name": "react-barebones-calendar",
  "version": "0.1.2",
  "private": false,
  "author": "George Vaughan <georgevaughandev@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/GeorgeVaughan/react-barebones-calendar"
  },
  "homepage": "https://GeorgeVaughan.github.io/react-barebones-calendar",
  "files": ["lib/"],
  "keywords": ["react-component", "react", "calendar", "custom"],
  "dependencies": {
    "moment": "^2.19.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-scripts": "1.0.17"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "gh-pages": "^1.1.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "build-lib":
      "npm run clean-lib && babel src --out-dir lib && node-sass-chokidar --include-path src/ src/ -o lib",
    "clean-lib": "rimraf lib",
    "start": "npm-run-all -p start-js start-css",
    "start-js": "react-scripts start",
    "start-css":
      "npm run build-css && node-sass-chokidar --include-path src/ src/ -o src/ --watch",
    "build": "npm-run-all build-js build-css",
    "build-js": "react-scripts build",
    "build-css": "node-sass-chokidar --include-path src/ src/ -o src/",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build&&gh-pages -d build"
  }
}
